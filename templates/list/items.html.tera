<table>
    <thead>
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>Store</th>
        </tr>
    </thead>
    <tbody>
        {% for item in items %}
        <tr>
            <td><input type="number" value="{{ item.qty }}"></td>
            <td>{{ item.name }}</td>
            <td>{{ item.store }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>
